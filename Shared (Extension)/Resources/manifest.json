{
    "manifest_version": 2,
    "default_locale": "en",
    "name": "URL Linker",
    "description": "A tool to copy webpage's title and URL in your favorite format.",
    "version": "1.1",
    "icons": {
        "128": "images/icon-128.png",
        "256": "images/icon-256.png",
        "512": "images/icon-512.png"
    },
    "content_scripts": [{
        "js": ["content.js"],
        "matches": ["*://*/*"]
    }],
    "permissions": []
}
